<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="50dp" />
            <stroke android:width="1dip" android:color="#031310" />
            <gradient android:angle="-90" android:startColor="#000000" android:endColor="#090000"  />
        </shape>
    </item>

    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="50dp" />
            <stroke android:width="3dip" android:color="#000000" />
            <gradient android:angle="-90" android:startColor="#292929" android:endColor="#1B1B1B" />
        </shape>
    </item>
</selector>